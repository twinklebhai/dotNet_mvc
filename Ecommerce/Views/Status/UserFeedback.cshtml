@model IEnumerable<Ecommerce.Models.FeedBack>

@{
    ViewData["Title"] = "UserFeedback";
}
<h1>UserFeedback</h1>
@if (Model.Count() > 0)
{
    
        <table class="table">
            <thead>
                <tr>
                    <th>
                        <h4> @Html.DisplayNameFor(model => model.ItemId)</h4>
                    </th>
                    <th>
                        <h4>  @Html.DisplayNameFor(model => model.Operation)</h4>
                    </th>
                    <th>
                        <h4> @Html.DisplayNameFor(model => model.Reason)</h4>
                    </th>
                    <th>
                        <h4> @Html.DisplayNameFor(model => model.Rating)</h4>
                    </th>




                </tr>
            </thead>
            <tbody>
                @foreach (var items in Model)
                {
                <tr>
                    <td>
                        @items.ItemId
                    </td>
                    <td>
                        @items.Operation
                    </td>
                    <td>
                        @items.Reason
                    </td>
                    <td>
                        @items.Rating
                    </td>
                </tr>
                }
                </tbody>
</table>




    <a asp-action="Edit">Back to List</a>

}


                
